<!--components/w-swiper/w-swiper.wxml-->
<!--展示轮播图-->
<swiper class="swiper" circular autoplay interval="5000" duration="300" indicator-dots indicator-active-color="#2184E3">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item}}" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>
