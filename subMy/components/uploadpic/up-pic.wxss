.picture_list {
  padding: 0 20rpx 10rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
  align-content: flex-start;
}
.picture_item {
  position: relative;
  margin-left: 12px;
  width: 82px;
  height: 82px;
}

.picture_item .del {
  position: absolute;
  top: 0;
  right: 0;
}
.picture_item image {
  width: 100%;
  height: 100%;
}
