<view class="coupon">
  <scroll-view scroll-top="{{scrollTop}}" scroll-y="true" class="list" bindscrolltolower="bindDownLoad" bindscroll="scroll" bindscrolltoupper="refresh">
    <view class="imgItem">
      <image class="coupon-bg" src="/assets/images/profile/yes-coupon.png"></image>
      <view class="content">
        <view class="price">
          <view class="price-box">
            ¥<text>300</text>
          </view>

        </view>
        <view class="cut"></view>
        <view class="shop">
          <view class="title">
            <image class="title-img" src="{{}}" />
            <view class="title-nav">星光灯具店</view>
          </view>
          <view class="use">
            <text>10月特惠</text>
          </view>
          <view class="time">截止时间：
            <text>2019/08/09 18：90</text>
          </view>
        </view>
        <view class="or-use">
          <text>进店使用</text>
        </view>
      </view>
    </view>
