/* subMy/pages/myPublicDetail/myPublicDetail.wxss */
/* *大师介绍 开始 */

.master-info {
  padding: 0 32rpx;
  box-sizing: border-box;
  margin-top: 32rpx;
  margin-bottom: 64rpx;
  display: flex;
  justify-content: flex-start;
}

.master-title {
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
}

.more {
  height: 32rpx;
  display: flex;
  justify-content: space-between;
}

.more>text {
  font-size: 26rpx;
  font-weight: 400;
  color: #999;
  line-height: 36rpx;
}

.icon-more {
  width: 12rpx;
  height: 24rpx;
  padding-top: 8rpx;
  /* padding-right: 40rpx; */
}

.master-info .left {
  margin-right: 26rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.master-info .left .img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.master-info .middle {
  width: calc(100% - 126rpx);
}

image {
  width: 100%;
  height: 100%;
}

.master-info .top {
  display: flex;
  justify-content: space-between;
}

.master-img {
  margin-top: 24rpx;
  display: flex;
  justify-content: space-between;
}

.master-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.center {
  color: rgba(102, 102, 102, 1);
}

.center .tag {
  margin-left: 10rpx;
  height: 40rpx;
  box-sizing: border-box;
  padding: 5rpx 11rpx;
  background: rgba(74, 108, 255, 0.14);
  border-radius: 8rpx;
  color: rgba(74, 108, 255, 1);
}

.center .name {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
}

.botm {
  padding: 5rpx 11rpx;
  margin-left: 10rpx;
  box-sizing: border-box;
  background-image: url('data:image/png;base64,');
  background-size: cover;
  background-position: center center;
  width: 148rpx;
  height: 40rpx;
  text-align: center;
}

.bot-phone {
  width: 66rpx;
  height: 66rpx;
}

.botm >text {
  width: 50rpx;
  height: 50rpx;
  font-size:22rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  line-height:30rpx;
}



.introduce {
  margin-top: 24rpx;
}

.master-img .img {
  width: 180rpx;
  height: 179rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 8px;
}

.master-img .img:nth-child(2) {
  margin: 0 10rpx;
}