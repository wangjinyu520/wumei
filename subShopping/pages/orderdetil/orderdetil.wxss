/* pages/orderdetil/orderdetil.wxss */

.lable{
  height: 117px;
  display: flex;
  border-bottom: 1px solid #eee;
}
.image{
  width: 128px;
  height: 88px;
  margin-left: 10px;
  margin-top: 14px;
}
.image>image{
  width: 100%;
  height: 100%;
}

.content{
  margin-top: 18px;
  margin-right: 8px;
  margin-left: 10px;
}
.title{
  font-size: 30rpx;
  color: #000000;
  font-weight: Medium;
  align-items: baseline;
}
.active{
  font-size: 22rpx;
  color: #666666;
  font-weight: Regular;
  margin-top: 15px;
}


.address{
  height: 50px;
  display: flex;
  border-bottom: 11px solid #eee;
}
.localimg{
  width: 10px;
  height: 14px;
  margin-left: 13px;
  margin-top: 19px;
}
.local{
  font-size: 30rpx;
  color: #666666;
  margin-top: 17px;
  margin-left: 2px;
}



.message{
  font-size: 30rpx;
  color: #222222;
}
.infor{
  display: flex;
  height: 50px;
  border-bottom: 1px solid #eee;
  text-align: center;
  align-items: center;
}
.infor>view{
  margin-left: 14px;
 
}

.vip{
  height: 50px;
  display: flex;
  text-align: center;
  align-items: center;
  border-bottom: 5px solid #eee;
}
.vipticket{
  margin-left: 14px; 
}
.vipprice{
  margin-left: 17px;
}


.ticket{
  /* height: 239px; */
  border-bottom: 10px solid #eee;
}
.ticketnum{
  margin-left: 11px;
  margin-top: 9px;
}

.code{
 flex: 1;
  text-align: center;
  align-items: center;
}
.qrcode-tip{
  padding-bottom:10px; 
}
.ticketimg{
  width: 146px;
  height: 146px;
  background: #666666;
  margin-top: 22px;
}
.ticketcode{
  font-size: 24rpx;
  color: #666666;
  margin-top: 9px;
}


.order{
  height: 63px;
  border-bottom: 13px solid #eee;
  font-size: 22rpx;
  color: #666666;
  line-height: 19px;
}
.orderlist{
  margin-left: 14px;
  margin-top: 13px;
}

.btn{
  height: 73px;
  display: flex;text-align: center;align-items: center;
}
.cancel{
  width: 352px;
  height: 50px;
  background: #2184E3;
  border-radius: 8px;
}
.cancel text{
 font-size: 30rpx;
  color: #FFFFFF;
  align-items: center;
}