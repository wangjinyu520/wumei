/* subShopping/pages/masterDetail/masterDetail.wxss */

page {
  background: rgba(255, 255, 255, 1);
}

.top-bg {
  height: 218rpx;
  width: 100%;
}

image {
  width: 100%;
  height: 100%;
}

.top-bg image {
  filter: blur(7rpx);
  -webkit-filter: blur(7rpx);
  -moz-filter: blur(7rpx);
  -o-filter: blur(7rpx);
  -ms-filter: blur(7rpx);
}

.content {
  padding: 0 32rpx;
  box-sizing: border-box;
  width: 100%;
  position: relative;
  top: -30rpx;
  left: 0;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx 24rpx 0rpx 0rpx;
}

.content-top {
  display: flex;
  justify-content: space-between;
}

.content-top .left {
  position: relative;
  top: -80rpx;
  width: 170rpx;
  height: 170rpx;
  background-color: #ccc;
  border-radius: 50%;
  box-shadow: 0rpx 2rpx 16rpx 0rpx rgba(108, 108, 108, 0.5);
  border: 6rpx solid rgba(255, 255, 255, 1);
}

.content-top .left image {
  border-radius: 50%;
}

.botm {
  padding-top: 10rpx;
  box-sizing: border-box;
  background-image: url('data:image/png;base64,');
  background-size: cover;
  background-position: center center;
  width: 109rpx;
  height: 142rpx;
  text-align: center;
}

.botm .text {
  width: 50rpx;
  height: 50rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(74, 108, 255, 1);
  line-height: 28rpx;
}

.rongyao {
  font-size: 42rpx;
  font-weight: bold;
  color: rgba(74, 108, 255, 1);
  line-height: 59rpx;
}

.content-bot {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.content-bot .left {
  width: 60%;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.content-bot .right {
  margin-top: 60rpx;
  width: 40%;
}

.line {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 15rpx;
}

.name {
  font-size: 44rpx;
  font-weight: bold;
  color: rgb(44, 9, 9);
  line-height: 62rpx;
}

.sex {
  margin-left: 6rpx;
  text-align: center;
  width: 87rpx;
  height: 36rpx;
  background: rgba(74, 108, 255, 0.11);
  border-radius: 18rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(74, 108, 255, 1);
  line-height: 36rpx;
}

.line2 {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.line2 .cut {
  height: 24rpx;
  width: 2rpx;
  background-color: rgba(51, 51, 51, 1);
  margin: 10rpx;
}

.sex image {
  width: 19rpx;
  height: 19rpx;
}

.price {
  display: flex;
  align-items: baseline;
  justify-content: flex-end;
}

.price .rent {
  display: inline;
  font-size: 46rpx;
  font-weight: bold;
  color: rgba(255, 94, 39, 1);
  line-height: 40rpx;
}

.price .unit {
  display: inline;
  color: rgba(255, 94, 39, 1);
  font-size: 30rpx;
  line-height: 34rpx;
  font-weight: 600;
}

.evalute-box {
  align-items: center;
}

.evaluate {
  height: 30rpx;
}

.evaluate image {
  width: 30rpx;
  height: 30rpx;
  margin: 0 4rpx;
}

.evaluate image:nth-last-child(1) {
  margin-right: 15rpx;
}

.people {
  line-height: 40rpx;
}

.scord {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.scord .value {
  margin-right: 26rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 33rpx;
}

.scord .value text {
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
}

.null {
  width: 100%;
  height: 16rpx;
  background: rgba(246, 246, 246, 1);
}

.master-tab {
  padding: 0 32rpx;
  box-sizing: border-box;
  width: 100%;
  height: 92rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.master-title {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.more {
  height: 32rpx;
  display: flex;
  justify-content: space-between;
}

.more>text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.icon-more {
  width: 12rpx;
  height: 24rpx;
  padding-top: 8rpx;
  /* padding-right: 40rpx; */
}

/* 导航栏切换效果 */

.top_nav {
  border-bottom: 1px solid #efefef;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.nav-name {
  display: inline-block;
  color: rgba(51, 51, 51, 1);
  padding: 18rpx 0;
  margin-right: 68rpx;
  font-size: 30rpx;
}

.nav-name .number {
  font-size: 40rpx;
  font-weight: bold;
  line-height: 56rpx;
}

.nav-name .name {
  font-size: 24rpx;
  font-weight: bold;
  line-height: 33rpx;
}

.nav-hover {
  color: #4a6cff;
  border-bottom: 4rpx solid #4a6cff;
}

.nav-hover .name {
  color: #4a6cff;
}

/* 大师图片，案例内容 */

.master-content {
  width: 100%;
  padding: 0 32rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.img-left {
  width: 48%;
}

.img-right {
  width: 48%;
}

.img-item {
  width: 100%;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 3rpx 9rpx 0rpx rgba(0, 0, 0, 0.14);
  border-radius: 12rpx;
  margin-bottom: 26rpx;
}

.img-item image {
  width: 100%;
  height: 296rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 12rpx 12rpx 0rpx 0rpx;
}

.img-item-bot {
  padding: 10rpx 25rpx 20rpx;
  box-sizing: border-box;
}

.img-item .text {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.xinxin image {
  width: 22rpx;
  height: 23rpx;
}

.xinixn {
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 30rpx;
}
