/* pages/rent/childCpns/w-select/w-select.wxss */
.com-selectBox{
    width: 70px;
    height: 24px;
    text-align: center;
    align-items: center;
}
.com-sContent{
    font-size: 30rpx;
    position: relative;
    height: 30px;
    line-height: 30px;
}
.com-sImg{
    position: absolute;
    right: 7px;
    top: 7px;
    width: 8px;
    height: 14px;
    align-items: center;
}
.com-sTxt{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding:0 10px 0 6px;
    font-size: 30rpx;
    text-align: center;
    align-items: center;
}
.com-sList{
    background: white;
    width: inherit;
    position: absolute;
    border: 1px solid #e2e2e2;
    border-top: none;
    box-sizing: border-box;
    z-index: 3;
    max-height: 120px;
    overflow: auto;
}
.com-sItem{
    height: 30px;
    line-height: 30px;
    border-top: 1px solid #e2e2e2;
    padding: 0 6px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 26rpx;
}
.com-sItem:first-child{
    border-top: none;
}