  width: 79rpx;
  height: 101rpx;
  text-align: center;
}

.botm .text {
  width: 50rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
}

.rongyao {
  font-size: 16rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 22rpx;
}

.introduce {
  margin-top: 24rpx;
}

.master-img .img {
  width: 180rpx;
  height: 179rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 8px;
}

.master-img .img:nth-child(2) {
  margin: 0 10rpx;
}

/* 热门活动开始 */

.activity {
  padding: 0 32rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  overflow: hidden;
}

.activity-info {
  margin-bottom: 21rpx;
  overflow: hidden;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 4px 16px 0px rgba(201, 201, 201, 0.5);
  border-radius: 8px;
  /* border: 1px solid rgba(209, 209, 209, 1); */
}

.activity-info .img {
  height: 334rpx;
  background-color: rgba(216, 216, 216, 1);
  border-radius: 8px;
}

.activity-info .title {
  margin-top: 21rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
}

.activity-info .area {
  margin-top: 4rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 33rpx;
}

.activity-info .bottom {
  padding: 0 32rpx;
  box-sizing: border-box;
}

.activity-info .btn {
  margin-top: 4rpx;
  margin-bottom: 21rpx;
  display: flex;
  justify-content: space-between;
}

.btn-img {
  width: 160rpx;
  height: 54rpx;
}

.btn text {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(255, 94, 39, 1);
  line-height: 40rpx;
}

/* 地址显示 */

.current {
  /* background: #e0e0e0; */
  color: #4a6cff;
}

.quyu {
  position: absolute;
  width: 100%;
  /* 区域的筛选：默认状态下不显示 */
  display: none;
  height: 60%;
  z-index: 999;
}

.quyu .qy {
  overflow-y: scroll;
  float: left;
  width: 100%;
  /* height: 450rpx; */
  height: 100%;
  line-height: 80rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #717273;
  /* border-right: 1rpx solid #f4f4f4; */
}

.qy view {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  padding-left: 45rpx;
  /* border-bottom: 1rpx solid #f4f4f4; */
}

.quyu-left {
  background: #fff;
}

.quyu-center {
  background: rgba(249, 249, 249, 1);
}

.quyu-right {
  background: rgba(249, 249, 249, 1);
}

.quyu-right view {
  display: flex;
  align-items: center;
}

.quyu-right image {
  width: 34rpx;
  height: 34rpx;
  margin-left: 30rpx;
}

.current {
  position: relative;
  /* background: #e0e0e0; */
  color: #4a6cff;
  background: rgba(249, 249, 249, 1);
}

.current:after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  background-color: #4a6cff;
  height: 100%;
  width: 6rpx;
}

.current2 {
  /* background: #fff; */
  color: #4a6cff;
}

.current3 {
  /* background: #fff; */
  color: #4a6cff;
}

.fullbg {
  position: fixed;
  top: 132rpx;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: rgb(1, 1, 1);
  transition: all 2s;
  opacity: 0;
}

.fullopacity {
  opacity: 0.5;
}

.nav-son.active .content {
  color: #4a6cff;
}

.nav-son.active .icon {
  border: 10rpx solid transparent;
  border-top: 10rpx solid #4a6cff;
}

@keyframes slidown {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0%);
  }
}

.slidown {
  display: block;
  animation: slidown 0.7s ease-in both;
}

@keyframes slidup {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(-100%);
  }
}

.slidup {
  display: block;
  animation: slidup 0.7s ease-in both;
}

.disappear {
  display: none;
}

button::after {
  border: none;
}

button {
  background-color: red;
  width: 50%;
  border-radius: 0;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.form-btn {
  width: 100%;
  display: flex;
  height: 98rpx;
}

.btn-reset {
  background-color: #f9f9f9;
}

.btn-submit {
  background-color: #4a6cff;
  color: #fff;
}

.sliderHView {
  margin: 0 40rpx 0 40rpx;
  height: 250rpx;
  width: 70%;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  background-color: #fff;
}

.valueSty {
  font-size: 30rpx;
  text-align: center;
  color: #999;
  margin-top: 15rpx;
}

.showMoney text {
  margin-right: 30rpx;
}

.twoSlider {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
