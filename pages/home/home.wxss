}

.master-info .top {
  display: flex;
  justify-content: space-between;
}

.master-img {
  margin-top: 24rpx;
  display: flex;
  justify-content: space-between;
}

.center {
  color: rgba(102, 102, 102, 1);
}

.center .tag {
