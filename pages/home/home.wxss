/* pages/index/index.wxss */

/*搜索框*/

/* 搜索栏部分展示 */

/* 左边部分开始 */

page {
  background: rgba(242, 242, 242, 1);
  width: 100%;
  font-family: PingFang-SC-Bold, PingFang-SC;
}

.searchBox {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
  height: 70rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.map {
  width: 25%;
}

.map .text image {
  margin-left: 10rpx;
  width: 22rpx;
  height: 13rpx;
}

.map.text text {
  width: 64rpx;
  height: 45rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
}

.map-item {
  padding: 0 64rpx;
  box-sizing: border-box;
  line-height: 60rpx;
  height: 60rpx;
}

.search-left {
  width: 455rpx;
  height: 70rpx;
  background-color: #fff;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 3rpx 9rpx 0px rgba(0, 0, 0, 0.14);
  border-radius: 35rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular, PingFang-SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.search-icon {
  font-size: 26rpx;
  margin-right: 10rpx;
}

.topub {
  margin-right: 30rpx;
  width: 15%;
  text-align: right;
}

.topub .img {
  width: 32rpx;
  height: 32rpx;
}

.map-list-open {
  position: fixed;
  top: 0px;
  left: 0;
  width: 100%;
  background-color: #fff;
  z-index: 999;
  animation: mapopen 0.5s;
}

.mask {
  position: fixed;
  top: 0px;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.mask-top {
  border-top: 2px solid #f9f9f9;
  background-color: #fff;
  height: 150rpx;
}

.map-list-close {
  position: fixed;
  z-index: 999;
  left: 0;
  width: 100%;
  background-color: #fff;
  animation: mapclose 0.5s;
}

@keyframes mapopen {
  from {
    top: -500px;
  }

  to {
    top: 0px;
  }
}

@keyframes mapclose {
  from {
    top: 0px;
  }

  to {
    top: -500px;
  }
}

/* 搜索栏部分展示结束 */

/* 、轮播图 */

.swipers {
  padding: 0 32rpx;
  box-sizing: border-box;
  width: 100%;
  height: 190rpx;
  border-radius: 12rpx;
  overflow: hidden;
  /* background-color: red; */
}

/*数据展示*/

/*icon*/

.icons {
  padding: 20rpx 32rpx;
  margin-top: 10rpx;
  box-sizing: border-box;
}

.icon-classify {
  width: 100%;
}

.icon-icon {
  width: 92rpx;
  height: 92rpx;
}

.text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: lighter;
  color: #666;
  line-height: 36rpx;
  margin-top: 10rpx;
  text-align: center;
}

.icon-top {
  width: 100%;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
