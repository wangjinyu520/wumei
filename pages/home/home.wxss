  font-size: 26rpx;
  font-family: PingFang-SC-Regular, PingFang-SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 37rpx;
}

.order-info-til {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
}

.order-info-area {
