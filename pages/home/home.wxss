  line-height: 50rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  font-size: 28rpx;
}

.master {
  background: rgba(255, 255, 255, 1);
  padding: 0 32rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.master-tab {
  width: 100%;
  height: 92rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/*大师介绍 开始 */

.master-info {
  margin-top: 32rpx;
