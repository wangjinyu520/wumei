/**app.wxss**/
@import 'assets/iconfont.wxss';
@import "icon.wxss";
@import "main.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
}

page {
  font-family: PingFang-SC-Bold, PingFang-SC;
}

.overhidden {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.overhidden1 {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}

.null {
  background: #f6f6f6;
  height: 20rpx;
}

.else {
  position: fixed;
  top: 132rpx;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 1);
}
button::after{
  border:none;
}
.else .else-img {
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translateX(-50%);
  width: 250rpx;
}

.else .else-img>image {
  width: 218rpx;
  height: 185rpx;
  margin: 0 auto;
}

.else .else-img>.else-text {
  margin-top: 20rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 42rpx;
}

.tip {
  text-align: center;
  color: #d8d8d8;
  font-size: 14px;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
}
