/**app.wxss**/
@import 'assets/iconfont.wxss';
@import "icon.wxss";
@import "main.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
}

page {
  font-family: PingFang-SC-Bold, PingFang-SC;
}

.overhidden {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.overhidden1 {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
